  <ItemGroup>
    <None Update="Assets\UI\playfieldDefaultBackground.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="Assets\UI\playscreenOverlay.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="Assets\UI\[ Pink Dreams ].png">
